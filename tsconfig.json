{
    "compilerOptions": {
        "target": "ES5",
        "module": "CommonJS",
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noUnusedParameters": true
    },
    "include": ["src/*.ts"]
}
